{
  "name": "@bufferapp/session-service",
  "version": "0.0.1",
  "description": "Buffer session service",
  "main": "index.js",
  "scripts": {
    "start": "micro",
    "lint": "eslint . --ignore-pattern coverage node_modules",
    "test": "npm run lint && jest --coverage",
    "test-watch": "jest --watch",
    "test-update": "jest -u"
  },
  "jest": {
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bufferapp/session-service.git"
  },
  "keywords": [
    "buffer",
    "session",
    "service"
  ],
  "author": "hharnisc@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bufferapp/session-service/issues"
  },
  "homepage": "https://github.com/bufferapp/session-service#readme",
  "dependencies": {
    "@bufferapp/micro-rpc": "0.1.0",
    "ioredis": "2.5.0",
    "jsonwebtoken": "7.3.0",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "micro": "7.3.3",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "jest": "19.0.2"
  }
